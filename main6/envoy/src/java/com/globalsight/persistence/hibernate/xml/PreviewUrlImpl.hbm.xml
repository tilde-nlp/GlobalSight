<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.globalsight.cxe.entity.previewurl.PreviewUrlImpl"
        table="URL_LIST">
        <id name="id" column="ID" type="long">
            <generator class="native" />
        </id>
        <property name="description" length="4000" column="DESCRIPTION"
            type="string" not-null="false" unique="false" />
        <property name="name" length="40" column="NAME" type="string"
            not-null="true" unique="false" />
        <property name="urlXml" length="4000" column="XML_URL"
            type="string" not-null="false" unique="false" />
    </class>
</hibernate-mapping>