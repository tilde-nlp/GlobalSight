      <!-- the object list component definition -->
      <component id="homecabinet_streamline" extends="homecabinet_drilldown:custom/ambassador/config/navigation/homecabinet/homecabinet_drilldown_component.xml">
         
         <!-- Description (not NLS'd) -->
